<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:c="sap.ui.core" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form"
	xmlns:ui="sap.ui" xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
	<Dialog title="Add new bank">
		<f:SimpleForm id="BankForm" maxContainerCols="2" editable="true" layout="ResponsiveGridLayout" labelSpanL="2" labelSpanM="2"
			emptySpanL="0" emptySpanM="0" columnsL="1" columnsM="1" class="editableForm sapUiSizeCompact">
			<f:content>
				<Label text="Bank Key"/>
				<Input id="BankKey" type="Number" placeholder="Enter bank key" maxLength="9"/>
				<Label text="Bank Name"/>
				<Input id="BankName" placeholder="Enter bank name"/>
				<Label text="Street"/>
				<Input id="Street" placeholder="Enter street"/>
				<Label text="City"/>
				<Input id="City" placeholder="Enter city"/>
				<Label text="Region"/>
				<Input id="Region" placeholder="Enter region"/>
				<Label text="Bank Country"/>
				<Input id="Country" placeholder="Enter bank country"/>
				<Label text="Bank number"/>
				<Input id="BankNumber" placeholder="Enter banke number"/>
				<Label/>
				<Button text="Save" type="Accept" press="handleSaveBtnBook"></Button>
				<Button text="Cancel" type="Reject" press="handleCancelBtnPress"></Button>
			</f:content>
		</f:SimpleForm>
	</Dialog>
</core:FragmentDefinition> 
 
